# API Configuration
REACT_APP_API_URL=http://localhost:3000

# Environment
REACT_APP_ENV=development

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id

# Sentry (Optional)
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false

# External Services
REACT_APP_SUPPORT_EMAIL=support@example.com
REACT_APP_DOCUMENTATION_URL=https://docs.example.com

# Performance
REACT_APP_CACHE_TTL=3600000
REACT_APP_MAX_REFERENCES=100

# UI Configuration
REACT_APP_DEFAULT_CITATION_STYLE=apa
REACT_APP_SHOW_DEVELOPER_TOOLS=false
